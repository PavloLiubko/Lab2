#include <iostream>
#include <cmath>


int main()
{
    setlocale(LC_ALL, "RU"); // українського кодування на жаль не знайшов 

    int x, y;

    std::cout << "Введіть значення х:";
    std::cin >> x;

    y = pow(x, 3) + 9 * pow(x, 2) - 16 * x + 36 + cos(x);

    std::cout << "Значення функції у при вашому х = " << y;

    return 0;
}
